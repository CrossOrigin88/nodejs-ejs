<body>
    <h1>User list:</h1>
    <% if (users.length > 0) { %>
        <ul>
            <% for (let user of users) { %>
                <li>Name : <%= user.name %> | Age : <%= user.age %></li>
            <% } %>
        </ul>
    <% } else { %>
        <h3>No member yet</h3>
    <% } %>
<%- include('../includes/end.ejs') %>